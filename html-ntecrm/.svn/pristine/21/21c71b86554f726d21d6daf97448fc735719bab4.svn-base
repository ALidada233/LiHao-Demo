          </el-form-item>
          <el-form-item label="供应商性质" :label-width="formLabelWidth" prop="nature">
            <el-select v-model="supplierForm.nature" placeholder="请选择供应商性质">
              <el-option label="原厂家" :value="1"></el-option>
