              <span slot="reference" style="cursor:default;" v-html="$root.OmissionText(scope.row.openBankAddress==null?null:scope.row.openBankAddress,12)"></span>
            </el-popover>
          </template>
        </el-table-column>
        <el-table-column prop="openBankTel" label="开户电话" align="center" width="150">
        </el-table-column>
