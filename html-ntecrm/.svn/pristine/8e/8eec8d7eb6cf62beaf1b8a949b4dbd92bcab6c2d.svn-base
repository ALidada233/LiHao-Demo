        } else if (to.path == "/admin/system") {
          this.navName = "系统管理"
        } else if (to.path == "/admin/discountApp") {
          this.navName = "折扣申请"
        } else if (to.path == "/admin/order") {
          this.navName = "订单管理"
        } else if (to.path == "/admin/jurisdiction") {
          this.navName = "权限设置"
        } else if (to.path == "/admin/TechnicalSupport") {
          this.navName = "技术工作台"
        } else if (to.path == "/admin/brand") {
          this.navName = "品牌"
        }else if (to.path == "/admin/supplier") {
          this.navName = "供应商管理"
        }else if (to.path == "/admin/category") {
          this.navName = "商品分类"
        }
      }
    },
    mounted() {
      var path = this.$store.state.asideDefaultActive;
      if (path == "home") {
        this.navName = null;
      } else if (path == "customer") {
        this.navName = "客户管理"
      } else if (path == "goods") {
        this.navName = "商品管理"
      } else if (path == "operator") {
        this.navName = "管理员管理"
      } else if (path == "role") {
        this.navName = "角色管理"
      } else if (path == "region") {
        this.navName = "区域管理"
      } else if (path == "offerList") {
        this.navName = "需求管理"
      } else if (path == "system") {
        this.navName = "系统管理"
      } else if (path == "discountApp") {
        this.navName = "折扣申请"
      } else if (path == "order") {
        this.navName = "订单管理"
      } else if (path == "jurisdiction") {
        this.navName = "权限设置"
      }else if (path == "TechnicalSupport") {
        this.navName = "技术工作台"
      }else if (path == "brand") {
        this.navName = "品牌"
      }else if (path == "supplier") {
          this.navName = "供应商管理"
      }else if (path == "category") {
          this.navName = "商品分类"
      }
    }
  }

</script>
