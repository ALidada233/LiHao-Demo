             message:"只能输入大于0的金额"
          }],
          billReceiptName:{
            required: true,
            trigger: 'blur',
            message: "收票人姓名不能为空"
          },
          billReceiptPhone:[{
            required: true,
            trigger: 'blur',
            message: "收票人手机号不能为空"
          },
          {
            trigger: 'blur',
            pattern: /^(13[0-9]|14[579]|15[0-3,5-9]|16[6]|17[0135678]|18[0-9]|19[89])\d{8}$/,
            message: "请输入正确的格式"
          }],
          file:{
            required: true,
            trigger: 'blur',
            message: " "
          }
        },
        recordList: null,
        form: null,
        moneyStatusSty: true,
        arrivalTimeSty: true,
        billSty: true,
        wlSty: true,
        OtherSty: true,
        orderOptLog: [],
        orderPayLog:[], //支付记录
        roleId: JSON.parse(sessionStorage.getItem('userDto')).roleId || null,
        activeTabs: 'first',
        technologyList: {
          afterSaleList: [],
          debugList: []
        },
        shippingVisible: false, //发货单页面
        cancelVisible:false,     //作废页面
        sendListVisible:false,     //发货单预览
        financeVisible:false, //新增财务订单
        invoiceVisible:false, //新增发票
        invoiceStatus:false,  //发票处于的状态（新增，编辑）
        invoiceFileVisible:false,//预览发票文件
        invoiceFile:null,//预览发票文件
        financeForm:{       //新增财务数据
          type:1
        },
        orderInvoice:[],    //新增发票数据
        orderInvoiceForm:{       //新增发票数据
        },
        fileStatus:1, //选中上传文件类型
        fileList: [], //上传图片
        fileListPdf: [], //上传pnf   
        goodsImg:null,
        file:null,  //上传成功返回路径
        selFile:null      //选中的发货单
      }
    },
    watch:{
      'rowId': function () {
        this.queryOrderDetail()
      },
      invoiceVisible(status){
        if(!status){
          this.orderInvoiceForm={};
          this.fileStatus=1 //选中上传文件类型
          this.fileList= [] //上传图片
          this.fileListPdf= [] //上传pnf   
          this.goodsImg=null
          this.file=null  //上传成功返回路径
        }
      },
      financeVisible(status){
        if(!status){
          this.financeForm={
            type:1
          }
        }
      }
    },
    methods: {
      tabsHandle(){

      },
      invoiceFileVisibleClose(){
        this.invoiceFileVisible=false;
      },
      handleRemoveFile(file, fileList) {
        this.file = null;
      },
      handleRemoveImage(file, fileList) {
        this.goodsImg = null;
      },
      handlePreview(file) {
        //console.log(file);
      },
      handleExceed(files, fileList) {
        this.$message.warning('当前只允许传输一个文件');
      },
      beforeRemove(file, fileList) {
        return this.$confirm(`确定移除 ${ file.name }？`);
      },
      successUploadImage(response, file, fileList) {
        this.fileList=[{name:response.data.url,url:response.data.url}];
        this.goodsImg = response.data.url;
        this.file= response.data.url;
      },
      successUploadFile(response, file, fileList) {
        this.fileListPdf=[{name:response.data.url,url:response.data.url}];
        this.file = response.data.url;
      },
      validateNum(text){ //验证
        var reg=/^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*)|0)$/;
        var pattern=new RegExp(reg);
        if(!pattern.test(text)){
          this.$message('请输入正确的金额');
        }
        return pattern.test(text)
      },
      resetHandle() {
        this.recovery();
        this.orderDetailList = this.recordList;
        //this.activeTabs="first";
      },
      recovery() {
        this.moneyStatusSty = true;
        this.arrivalTimeSty = true;
        this.billSty = true;
        this.wlSty = true;
        this.OtherSty = true;
      },
      sendOrder(updateType, type) {
        var parmas = {};
        parmas.id = this.rowId;
        if (updateType == 2) { //销售人员
          parmas.updateType = 2
          if (type == 0) { //发货
            parmas.type = 0;
            // parmas.debugRequirement = this.orderDetailList.order.debugRequirement //调试要求,
            parmas.shippingStatus = this.orderDetailList.order.shippingStatus //0未到货1到货未发货2已发货3确认收货
            parmas.signForTime = this.orderDetailList.order.signForTime //签收时间
            parmas.sendTime = this.orderDetailList.order.sendTime //发货时间
            parmas.deliveryTime = this.orderDetailList.order.deliveryTime //货期
            parmas.orderStatus= this.orderDetailList.order.orderStatus //订单状态

          } else if (type == 1) { //发票
            parmas.type = 1;
            parmas.invoice = this.orderDetailList.order.invoice //发票状态0不开发票1开发票
            parmas.billAddress = this.orderDetailList.order.billAddress //收发票地址
            parmas.billLogBank = this.orderDetailList.order.billLogBank //收票物流公司
            parmas.billLogNo = this.orderDetailList.order.billLogNo //收票物流单号
            parmas.billReceiptName = this.orderDetailList.order.billReceiptName //收票人姓名
            parmas.billReceiptPhone = this.orderDetailList.order.billReceiptPhone //收票人手机号
            parmas.billPostNo = this.orderDetailList.order.billPostNo //收票人邮政编码
            parmas.isSendBill = this.orderDetailList.order.isSendBill //是否寄出发票0否1是
            parmas.sendBillTime = this.orderDetailList.order.sendBillTime //发票寄出时间
          } else if (type == 2) {
            // parmas.type = 2;
            // parmas.address = this.orderDetailList.order.address //收货地址
            // parmas.receiptName = this.orderDetailList.order.receiptName //收货人姓名
            // parmas.receiptPhone = this.orderDetailList.order.receiptPhone //收货人手机
            // parmas.postNo = this.orderDetailList.order.postNo //邮政编码
            // parmas.logBank = this.orderDetailList.order.logBank //物流公司
            // parmas.logNo = this.orderDetailList.order.logNo //物流单号
          }
        } else if (updateType == 1) { //采购
          parmas.updateType = 1;
          parmas.preArrivalTime = this.orderDetailList.order.preArrivalTime; //预交货日期，
          parmas.arrivalTime = this.orderDetailList.order.arrivalTime; //到货日期
        } else if (updateType == 3) { //财务(42)总助（37）超级管理员（36）
          parmas.updateType = 3;
          parmas.payTime = this.orderDetailList.order.payTime; //支付时间
          parmas.prePayTime = this.orderDetailList.order.prePayTime; //预付时间
          parmas.freightMoney = this.orderDetailList.order.freightMoney; //运费
          parmas.payMoney = this.orderDetailList.order.payMoney; //支付金额（尾款）
          parmas.preMoney = this.orderDetailList.order.preMoney; //待付金额
          parmas.preTradeNo = this.orderDetailList.order.preTradeNo; //预付流水号
          parmas.payStatus = this.orderDetailList.order.payStatus; //支付状态
          // parmas.moneyStatus = this.orderDetailList.order.moneyStatus; //金额状态
          if(this.validateNum(parmas.payMoney)&&this.validateNum(parmas.preMoney)){
            if((parmas.payMoney+parmas.preMoney)<=this.orderDetailList.order.totalMoney){
                this.$message({
                  message: '警告：支付的总金额不正确',
                  type: 'warning'
                });
                return 
            }
          }else{
            return
          }
        } else {
          return false
        }
        updateOrder(parmas, (res) => {
          if (res.msgCode == 200) {
            this.$message({
              message: res.msg,
              type: 'success'
            });
            const loading = this.$loading({
              lock: true,
              target: '.detail',
              text: '重载中',
              spinner: 'el-icon-loading',
              background: 'rgba(255, 255, 255, 0.9)'
            });
            new Promise((resolve, reject) => {
              queryOrderDetail({
                id: this.rowId
              }, (res) => {
                resolve(res)
              })
            }).then((res) => {
              if(res.data.order.invoiceFile){
                res.data.order.invoiceFile=res.data.order.invoiceFile.split(',')
              }else{
                res.data.order.invoiceFile=[]
              }
              this.orderDetailList = res.data;
              this.orderOptLog = JSON.parse(JSON.stringify(res.data.orderOptLog));
              this.recordList = JSON.parse(JSON.stringify(res.data));
              this.orderPayLog=JSON.parse(JSON.stringify(res.data.orderPayLog));
              this.orderInvoice=JSON.parse(JSON.stringify(res.data.orderInvoice));
              this.technologyList.afterSaleList = res.data.afterSaleList||JSON.parse(JSON.stringify(res.data.afterSaleList));
              this.technologyList.debugList = JSON.parse(JSON.stringify(res.data.debugList));
              this.recovery();
              loading.close();
            }).catch((err) => {
              this.$message({
                message: err,
                type: 'warning'
              });
            })
          } else {
            this.$message({
              message: res.msg,
              type: 'warning'
            });
          }
        })
      },
      queryOrderDetail(){
          const loading = this.$loading({
            lock: true,
            target: '.detail',
            text: '重载中',
            spinner: 'el-icon-loading',
            background: 'rgba(255, 255, 255, 0.9)'
          });
         queryOrderDetail({
          id: this.rowId
        }, (res) => {
          if(res.msgCode==200){
            if(res.data.order.invoiceFile){
              res.data.order.invoiceFile=res.data.order.invoiceFile.split(',')
            }else{
              res.data.order.invoiceFile=[]
            }
            this.orderDetailList = res.data;
            this.orderOptLog = JSON.parse(JSON.stringify(res.data.orderOptLog));
            this.recordList = JSON.parse(JSON.stringify(res.data));
            this.orderPayLog=JSON.parse(JSON.stringify(res.data.orderPayLog));
            this.orderInvoice=JSON.parse(JSON.stringify(res.data.orderInvoice));
            this.technologyList.afterSaleList = JSON.parse(JSON.stringify(res.data.afterSaleList));
            this.technologyList.debugList = JSON.parse(JSON.stringify(res.data.debugList));
          }
           loading.close();
        })
      },
      financeSubmit(){  //财务表单提交
        this.$refs.finance.validate(valid=>{
          if(valid){
            var parmas=Object.assign({orderId:this.rowId},this.financeForm);
            updateOrderFinance(parmas,res=>{
                if(res.msgCode==200){
                  this.$message({
                    message: res.msg,
                    type: 'success'
                  });
                  this.financeVisible=false;
                  this.queryOrderDetail();
                }else{
                  this.$message({
                    message: res.msg,
                    type: 'warning'
                  });
                }
            })
          }
        })
      },
      selInvoiceRow(row){
        this.invoiceStatus=true;
        this.invoiceVisible=true;
        this.orderInvoiceForm=row;
        if(row.file){
          if(row.file.indexOf('pdf')>-1){
            this.fileListPdf=[{name:row.file,url:row.file}];
            this.fileStatus=2;
          }else{
            this.fileList=[{name:row.file,url:row.file}]
            this.fileStatus=1;
          }
          this.file=row.file;
        }
      },
      invoiceSubmit(){
        if(!this.file){
          this.$message({
            message:'发票文件不能为空' ,
            type: 'warning'
          });
          return
        }
        this.$refs.invoice.validate(valid=>{
          if(valid){
            var parmas={
              orderId:this.orderRowList.id,
              file:this.file
            }
            var newParmas=Object.assign(this.orderInvoiceForm,parmas);
            if(this.invoiceStatus){
            }
            saveOrderInvoice(newParmas,res=>{
                if(res.msgCode==200){
                  this.$message({
                    message: res.msg,
                    type: 'success'
                  });
                  this.invoiceVisible=false;
                  this.queryOrderDetail();
                }else{
                  this.$message({
                    message: res.msg,
                    type: 'warning'
                  });
                }
            })
          }
        })
      }
    },
    components: {
      discuss: () => import('@/page/admin/order/components/discuss'),
      shippingList: () => import('@/page/admin/order/components/shippingList'),
      cancal:()=>import('@/page/admin/order/components/cancel')
    }
  }

</script>
