      loadExcel: "/nte-crm/purchaseListExport.do?",
      multipleSelection: []
    };
  },
  watch: {
    pdfVisible(status) {
      if (!status) {
        this.viewFile = "";
      }
    }
  },
  methods: {
    alertOP(item, index) {
      item.check = !item.check;
      this.$set(this.partTypeList[index], "check", item.check);
    },
    handleClose() {
      this.purchaseVisible = false;
      this.selRow = {
        supplierId: null,
        brandId: null
      };
      this.file = null;
      this.fileList = [];
      this.fileListPdf = [];
    },
    purchaseInquiryClose() {
      this.purchaseInquiryVisible = false;
      this.selPurchaseInquiry = null;
      this.purchaseInquiry = { status: 0 };
      this.file = null;
      this.fileList = [];
      this.fileListPdf = [];
    },
    handleSizeChange(pageSize) {
      //数据条数改变时
      this.loading = true;
      this.pageSize = pageSize;
      this.currentPage = 1;
      this.queryPurchaseOrderList();
    },
    handleCurrentChange(currentPage) {
      //选取分页
      this.loading = true;
      this.queryPurchaseOrderList();
    },
    getPurchaseOrderInfo(id, status) {
      getPurchaseInfo({ id: id, status: status }, res => {
        if (res.msgCode == "200") {
          res = res.data;
          this.partList = res.purchaseOrder.partList;
          var partTypeList = JSON.parse(
            JSON.stringify(res.purchaseOrder.partList)
          );
          this.partTypeList = partTypeList.map(item => {
            if (item.isTecSupport == 1) {
              item.check = true;
            } else {
              item.check = false;
            }
            return item;
          });
          setTimeout(() => {
            this.loading = false;
          }, 200);
        } else {
          setTimeout(() => {
            this.loading = false;
          }, 200);
        }
      });
    },
    queryPurchaseOrderList() {
      //查询列表
      var parmas = {
        pageSize: this.pageSize,
        page: this.currentPage
      };

      //搜索条件
      if (this.seatchType) {
        parmas.seatchType = this.seatchType;
      }
      //搜索内容
      if (this.keys) {
        parmas.keys = this.keys;
      }
      //创建时间
      if (this.createTime) {
        parmas.createTimeStart = this.createTime[0];
        parmas.createTimeEnd = this.createTime[1];
      }
      //修改时间
      if (this.updateTime) {
        parmas.updateTimeStart = this.updateTime[0];
        parmas.updateTimeEnd = this.updateTime[1];
      }
      if (this.status) {
        parmas.status = this.status;
      }
      if (this.currency) {
        parmas.currency = this.currency;
      }
      if (this.custBelong) {
        parmas.custBelong = this.custBelong;
      }
      if (this.orderCreateTime) {
        parmas.orderCreateTimeStart = this.orderCreateTime[0];
        parmas.orderCreateTimeEnd = this.orderCreateTime[1];
      }

      this.loading = true;
      queryPurchaseOrderList(parmas, res => {
        if (res.msgCode == "200") {
          res = res.data;
          this.purchaseList = res.list;
          this.pageCount = res.pageCount;
          this.$refs.table.bodyWrapper.scrollTop = 0;
          setTimeout(() => {
            this.loading = false;
          }, 200);
        } else {
          setTimeout(() => {
            this.loading = false;
          }, 200);
        }
      });
    },
    editPurchase(row) {
      var newRow = JSON.parse(JSON.stringify(row));
      if (newRow.supplierId == 0) {
        newRow.supplierId = "";
      }
      if (newRow.brandId == 0) {
        newRow.brandId = null;
      } else {
        newRow.brandId = Number(newRow.brandId);
      }
      if (newRow.price == 0) {
        newRow.price = null;
      }
      if (newRow.currency == 0) {
        newRow.currency = null;
      }
      this.selRow = newRow;
      this.purchaseVisible = true;
      if (row.file) {
        if (row.file.indexOf("pdf") > -1) {
          this.fileStatus = 2;
          this.fileListPdf = [{ name: row.file, url: row.file }];
        } else {
          this.fileStatus = 1;
          this.fileList = [{ name: row.file, url: row.file }];
        }
      }
      this.file = row.file;
      this.getPurchaseOrderInfo(row.id, row.status);
    }, //图片上传
    handleRemoveFile(file, fileList) {
      this.file = null;
    },
    handleRemoveImage(file, fileList) {
      this.goodsImg = null;
    },
    handlePreview(file) {
      //console.log(file);
    },
    handleExceed(files, fileList) {
      this.$message.warning("当前只允许传输一个文件");
    },
    beforeRemove(file, fileList) {
      return this.$confirm(`确定移除 ${file.name}？`);
    },
    successUploadImage(response, file, fileList) {
      this.fileList = [{ name: response.data.url, url: response.data.url }];
      this.goodsImg = response.data.url;
      this.file = response.data.url;
    },
    successUploadFile(response, file, fileList) {
      this.fileListPdf = [{ name: response.data.url, url: response.data.url }];
      this.file = response.data.url;
    },
    purchaseSub() {
      var selChk = this.partTypeList.filter(item => {
        return (item.check = true);
      });
      var parmas = {
        id: this.selRow.id,
        supplierId: this.selRow.supplierId,
        brandId: this.selRow.brandId,
        currency: this.selRow.currency,
        changeModel: this.selRow.changeModel || "",
        price: this.selRow.price,
        rate: this.selRow.rate,
        arrivalTime: this.selRow.arrivalTime || "", // 到货日期
        preArrivalTime: this.selRow.preArrivalTime || "", // 预到货日期
        invoiceTime: this.selRow.invoiceTime || "", //  票到日期
        payType: this.selRow.payType || {}, //  付款方式
        payInfo: this.selRow.payInfo || {}, //  付款情况
        contractNo: this.selRow.contractNo || {}, //  采购合同号
        file: this.file || {}, //  文件
        invoiceNo: this.selRow.invoiceNo || {}, //  发票号
        status: this.selRow.status, //  状态2完成3作废
        remark: this.selRow.remark
      };
      //作废的情况下不需要验证
      if (this.selRow.status == 2) {
        var parmas = {
          id: this.selRow.id,
          status: this.selRow.status, //  状态2完成3作废
          remark: this.selRow.remark
        };

        updatePurchaseOrder(parmas, res => {
          if (res.msgCode == 200) {
            this.$message({
              type: "success",
              message: res.msg
            });

            this.handleClose();
            this.queryPurchaseOrderList();
          } else {
            this.$message({
              type: "info",
              message: res.msg
            });
          }
        });
        return;
      }
      this.$refs.form.validate(valid => {
        if (valid) {
          updatePurchaseOrder(parmas, res => {
            if (res.msgCode == 200) {
              this.$message({
                type: "success",
                message: res.msg
              });
              this.handleClose();
              this.queryPurchaseOrderList();
            } else {
              this.$message({
                type: "info",
                message: res.msg
              });
            }
          });
        }
      });
    },
    purchaseSupport() {
      //转技术支持
      if (!this.selRow.preArrivalTime) {
        this.$message({
          message: "请选择预到货时间",
          type: "warning"
        });
        return;
      }
      //是否有辅件提醒
      var isPartArr = this.partTypeList.filter(item => {
        return item.check == true;
      });
      if (isPartArr == 0 && this.partTypeList.length > 0) {
        this.$confirm("您有可点中选取的辅件, 是否继续提交?", "技术支持", {
          confirmButtonText: "确定",
          cancelButtonText: "取消",
          type: "warning"
        })
          .then(() => {
            updatePurchaseOrder(
              {
                id: this.selRow.id,
                preArrivalTime: this.selRow.preArrivalTime,
                isTecSupport: 1
              },
              res => {
                if (res.msgCode == 200) {
                  this.$message({
                    type: "success",
                    message: res.msg
                  });
                  this.handleClose();
                  this.queryPurchaseOrderList();
                } else {
                  this.$message({
                    type: "warning",
                    message: res.msg
                  });
                }
              }
            );
          })
          .catch(() => {});
      } else {
        this.$confirm("此条商品内容将转技术支持, 是否继续提交?", "技术支持", {
          confirmButtonText: "确定",
          cancelButtonText: "取消",
          type: "warning"
        })
          .then(() => {
            var checkIdArr = isPartArr.reduce((item, next) => {
              item.push(next.id);
              return item;
            }, []);
            updatePurchaseOrder(
              {
                id: this.selRow.id,
                preArrivalTime: this.selRow.preArrivalTime,
                isTecSupport: 1,
                indexStr:
                  checkIdArr.length > 0 ? checkIdArr.join("|") + "|" : {}
              },
              res => {
                if (res.msgCode == 200) {
                  this.$message({
                    type: "success",
                    message: res.msg
                  });
                  this.handleClose();
                  this.queryPurchaseOrderList();
                } else {
                  this.$message({
                    type: "warning",
                    message: res.msg
                  });
                }
              }
            );
          })
          .catch(() => {});
      }
    },
    getBrandList() {
      //获取所有品牌
      getBrandList({ isShow: 1 }, res => {
        this.brandList = res.data.goodsList;
      });
    },
    getSupplierList() {
      //获取所有供应商
      getSupplierList(res => {
        this.supplierList = res.data.list;
      });
    },
    getSupChange(id, status) {
      //根据供应商ID查询品牌列表
      if (!id) {
        return;
      }
      if (status == "brandId") {
        var params = {
          brandId: id
        };
      } else {
        var params = {
          supplierId: id
        };
      }
      getSupplierBrandById(params, res => {
        if (res.msgCode != 200) {
          this.$message({
            type: "info",
            message: res.msg
          });
        } else {
          if (status == "brandId") {
            this.supplierList = res.data.list;
          } else {
            this.brandList = res.data.list;
          }
        }
      });
    },
    //修改预到货日期
    changePreArrivalTime(id, time, row) {
      updatePurchaseOrder(
        {
          id: id,
          preArrivalTime: time
        },
        res => {
          if (res.msgCode == 200) {
            this.$message({
              type: "success",
              message: res.msg
            });

            this.handleClose();
            this.queryPurchaseOrderList();
          } else {
            this.$message({
              type: "info",
              message: res.msg
            });
          }
        }
      );
    },
    //修改到货时间
    changeArrivalTime(id, time, row) {
      console.log(row);
      if (row.status == 2) {
        this.$message({
          type: "info",
          message: "采购订单标注为作废，不能操作"
        });
        return;
      }
      updatePurchaseOrder(
        {
          id: id,
          arrivalTime: time,
          status: 1
        },
        res => {
          if (res.msgCode == 200) {
            this.$message({
              type: "success",
              message: res.msg
            });

            this.handleClose();
            this.queryPurchaseOrderList();
          } else {
            this.$message({
              type: "info",
              message: res.msg
            });
          }
        }
      );
    },
    objectSpanMethod({ row, column, rowIndex, columnIndex }) {
      if (columnIndex === 3) {
        if (rowIndex % 2 === 0) {
          return {
            rowspan: 2,
            colspan: 1
          };
        } else {
          return {
            rowspan: 0,
            colspan: 0
          };
        }
      }
    },
    //选中导出数据
    handleSelectionChange(val) {
      this.multipleSelection = val;
    },
    //导出采购单
    exportAllGoods() {
      //查询列表
      var parmas = {};
      //勾选id
      if (this.multipleSelection.length > 0) {
        parmas.indexStr = this.multipleSelection.map(v => v.id).join(",");
      } else {
        parmas.indexStr = this.purchaseList.map(v => v.id).join(",");
      }
      if (!parmas.indexStr) {
        this.$message({
          type: "info",
          message: "数据还没加载完，请稍等..."
        });
        return;
      }
      //搜索条件
      if (this.seatchType) {
        parmas.seatchType = this.seatchType;
      }
      //搜索内容
      if (this.keys) {
        parmas.keys = this.keys;
      }
      //创建时间
      if (this.createTime) {
        parmas.createTimeStart = this.createTime[0];
        parmas.createTimeEnd = this.createTime[1];
      }
      //修改时间
      if (this.updateTime) {
        parmas.updateTimeStart = this.updateTime[0];
        parmas.updateTimeEnd = this.updateTime[1];
      }
      if (this.status) {
        parmas.status = this.status;
      }
      if (this.currency) {
        parmas.currency = this.currency;
      }
      if (this.custBelong) {
        parmas.custBelong = this.custBelong;
      }
      if (this.orderCreateTime) {
        parmas.orderCreateTimeStart = this.orderCreateTime[0];
        parmas.orderCreateTimeEnd = this.orderCreateTime[1];
      }
      purchaseListExport(parmas, res => {
        if (res.msgCode == 200) {
          this.$message({
            type: "success",
            message: res.msg
          });
          const a = document.createElement("a");
          document.body.appendChild(a);
          a.style.display = "none";
          const url = res.data.path;
          a.href = url;
          // // 指定下载的文件名
          a.download = "采购单数据";
          a.click();
          document.body.removeChild(a);
        } else {
          this.$message({
            type: "info",
            message: res.msg
          });
        }
      });
      // a.href = url + parmas;
      // console.log(url + parmas);
      // // 指定下载的文件名
      // a.download = "采购单数据";
      // a.click();
      // document.body.removeChild(a);
      // 移除blob对象的url
      //window.URL.revokeObjectURL(url);
    }
  },
  mounted() {
    this.tableHeight =
      window.innerHeight - this.$refs.table.$el.offsetTop - 150;

    this.getBrandList();
    this.getSupplierList();
    if (this.$route.query.orderId) {
      //消息跳转客户
      var parmas = {
        pageSize: this.pageSize, //每页数量
        page: this.currentPage, //当前页
        orderId: this.$route.query.orderId
      };
      this.loading = true;
      queryPurchaseOrderList(parmas, res => {
        if (res.msgCode == "200") {
          res = res.data;
          this.purchaseList = res.list;
          this.pageCount = res.pageCount;
          this.$refs.table.bodyWrapper.scrollTop = 0;
          setTimeout(() => {
            this.loading = false;
          }, 200);
        } else {
          setTimeout(() => {
            this.loading = false;
          }, 200);
        }
      });
    } else {
      this.queryPurchaseOrderList();
    }
    var currentUrl = window.location.href;
    var targetUrl = currentUrl.split("?")[0];
    window.location.href = targetUrl;
  }
};
</script>
