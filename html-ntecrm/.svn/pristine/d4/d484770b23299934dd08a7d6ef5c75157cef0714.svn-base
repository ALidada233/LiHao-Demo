<script type="text/javascript">
import {getContractCode,saveOfferFile} from "util/req/offerList/index"
	export default{
		name:"offerNaiteen",
		props:{
			naiteenContent:{
				type:Object  
			},
			id:{
				type:Number
			}
		},
		data(){
			return{
				content:{},	//合同内容
				fileList:[],
				weeksStart:6,
				weeksEnd:8,
				times:null
			}
		},
		watch:{
			
			naiteenContent(res){
				this.content=res;	
				//this.getContractCode();
			},
			weeksStart(val){
				this.$set(this.content,'weeksStart',val)
			},
			weeksEnd(val){
